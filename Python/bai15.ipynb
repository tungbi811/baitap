    "def find_all(string,substring):\n",
    "    lst = []\n",
    "    for i in range(len(string)):\n",
    "        if string[i:i+len(substring)] == substring:\n",
    "            lst.append(i)\n",
    "    return lst \n",
    "\n",
    "s = input(\"Enter a String: \") \n",
    "sub = input(\"Enter a Substring: \")\n",
    "print(find_all(s,sub))"
   ]
  }
